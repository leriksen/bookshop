= form_for @user_type do |f|
  - if @user_type.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_type.errors.count, "error")} prohibited this user_type from being saved:"
      %ul
        - @user_type.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :typename
    = f.text_field :typename
  .field
    = f.label :level
    = f.number_field :level
  .actions
    = f.submit 'Save'
